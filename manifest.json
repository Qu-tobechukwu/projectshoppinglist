self.addEventListener('install', (e) => {
  e.waitUntil(
    caches.open('stellies-cache').then((cache) => {
      return cache.addAll([
        '/',
        '/index.html',
        '/checkout.html',
        '/thankyou.html',
        '/merch.html',
        '/style.css',
        '/script.js',
        '/install.js',
        '/assets/logo.svg',
      ]);
    })
  );
});

self.addEventListener('fetch', (e) => {
  e.respondWith(caches.match(e.request).then((response) => response || fetch(e.request)));
});
